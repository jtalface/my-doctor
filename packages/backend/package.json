{
  "name": "@mydoctor/backend",
  "version": "1.0.0",
  "description": "Backend API for MyDoctor AI-powered medical assistant",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/server.ts",
    "dev:prod": "NODE_ENV=production nodemon --exec ts-node src/server.ts",
    "start": "NODE_ENV=production node dist/server.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mydoctor",
    "backend",
    "medical",
    "api"
  ],
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "openai": "^6.10.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
