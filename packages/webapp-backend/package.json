{
  "name": "@mydoctor/webapp-backend",
  "version": "1.0.0",
  "private": true,
  "description": "Backend API for MyDoctor Web Application",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:prod": "NODE_ENV=production tsx watch src/server.ts",
    "build": "tsc && mkdir -p dist/state && cp src/state/machine.json dist/state/machine.json",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "openai": "^4.24.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.5",
    "@types/uuid": "^9.0.7",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
